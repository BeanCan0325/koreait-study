CREATE TABLE memeber_notnull (
	id varchar(50) NOT NULL ,
	pwd varchar(250) NOT NULL,
	name varchar(50),
	age int,
	in_date datetime
);

INSERT INTO memeber_notnull
VALUES('test','qwer1234!',NULL, NULL,now());

INSERT INTO memeber_notnull(id,pwd,in_date)
VALUES ('test2', 'qwer1234!',now());
